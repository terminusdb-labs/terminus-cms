[
  { "@type" : "@context",
    "@base" : "terminusdb:///lego/data/",
    "@schema" : "terminusdb:///lego/schema#"
  },
  {
    "@type": "Class",
    "@id": "Part",
    "@key": {
      "@type": "Lexical",
      "@fields": [
        "part_number"
      ]
    },
    "part_number": "xsd:string",
    "category": "Category",
    "name": "xsd:string",
    "material": "Material"
  },
  {
    "@type": "Enum",
    "@id": "Material",
    "@value": [
      "Cardboard/Paper",
      "Cloth",
      "Foam",
      "Metal",
      "Plastic",
      "Rubber"
    ]
  },
  {
    "@type": "Enum",
    "@id": "Category",
    "@value": [
      "Bars, Ladders and Fences",
      "Baseplates",
      "Bricks Sloped",
      "Belville, Scala and Fabuland",
      "Bricks Curved",
      "Bricks Round and Cones",
      "Bricks Special",
      "Bricks Wedged",
      "Bricks",
      "Clikits",
      "Containers",
      "Duplo, Quatro and Primo",
      "Electronics",
      "Flags, Signs, Plastics and Cloth",
      "HO Scale",
      "Hinges, Arms and Turntables",
      "Large Buildable Figures",
      "Magnets and Holders",
      "Mechanical",
      "Minidoll Heads",
      "Minidoll Lower Body",
      "Minidoll Upper Body",
      "Minifig Accessories",
      "Minifig Heads",
      "Minifig Headwear",
      "Minifig Lower Body",
      "Minifig Upper Body",
      "Minifigs",
      "Modulex",
      "Non-Buildable Figures (Duplo, Fabuland, etc)",
      "Non-LEGO",
      "Other",
      "Panels",
      "Plants and Animals",
      "Plates Angled",
      "Plates Round Curved and Dishes",
      "Plates Special",
      "Plates",
      "Pneumatics",
      "Projectiles / Launchers",
      "Rock",
      "Stickers",
      "String, Bands and Reels",
      "Supports, Girders and Cranes",
      "Technic Axles",
      "Technic Beams Special",
      "Technic Beams",
      "Technic Bricks",
      "Technic Bushes",
      "Technic Connectors",
      "Technic Gears",
      "Technic Panels",
      "Technic Pins",
      "Technic Special",
      "Technic Steering, Suspension and Engine",
      "Tiles Round and Curved",
      "Tiles Special",
      "Tiles",
      "Tools",
      "Transportation - Land",
      "Transportation - Sea and Air",
      "Tubes and Hoses",
      "Wheels and Tyres",
      "Windows and Doors",
      "Windscreens and Fuselage",
      "Znap"
    ]
  },
  { "@id": "PartRelation",
    "@type": "Class",
    "relation_type": "RelationType",
    "left" : "Part",
    "right" : "Part"
  },
  {
    "@id": "RelationType",
    "@type": "Enum",
    "@value": [
      "Alternate",
      "Mold",
      "Pair",
      "Pattern",
      "Print",
      "Sub-Part"
    ]
  },
  {
    "@id": "Inventory",
    "@type": "Class",
    "version": "xsd:positiveInteger",
    "inventory_parts": {
      "@type": "Set",
      "@class": "InventoryPart"
    },
    "inventory_minifigs": {
      "@type": "Set",
      "@class": "InventoryMinifig"
    }
  },
  {
    "@type": "Class",
    "@id": "InventoryMinifig",
    "quantity": "xsd:positiveInteger",
    "minifig": "Minifig"
  },
  {
    "@type": "Class",
    "@id": "Minifig",
    "@key": {
      "@type": "Lexical",
      "@fields": ["figure_number"]
    },
    "img_url": "xsd:anyURI",
    "name": "xsd:string",
    "num_parts": { "@type" : "Optional", "@class": "xsd:positiveInteger"},
    "figure_number" : "xsd:string"
  },
  {
    "@id": "InventoryPart",
    "@type": "Class",
    "@key" : {"@type" : "Lexical",
              "@fields" : ["inventory_part_id"]},
    "@subdocument": [],
    "inventory_part_id": "xsd:string",
    "element": "Element",
    "quantity": "xsd:positiveInteger",
    "spare": "xsd:boolean"
  },
  {
    "@id": "Element",
    "@type": "Class",
    "part": "Part",
    "color": {
      "@type": "Optional",
      "@class": "Color"
    },
    "image_url": {
      "@type" : "Optional",
      "@class" : "xsd:anyURI"
    }
  },
  {
    "@id": "Color",
    "@type": "Class",
    "name": "xsd:string",
    "rgb": "xsd:string",
    "transparent": "xsd:boolean"
  },
  {
    "@id": "LegoSet",
    "@type": "Class",
    "name": "xsd:string",
    "year": "xsd:gYear",
    "theme": "Theme",
    "inventory_set": { "@type" : "Set",
                       "@class" : "InventorySet"}
  },
  {
    "@type": "Class",
    "@id": "InventorySet",
    "@subdocument": [],
    "@key" : {"@type" : "Random"},
    "inventory": "Inventory",
    "quantity": "xsd:positiveInteger"
  },
  {
    "@id": "Theme",
    "@type": "Class",
    "@key": {
      "@type": "Lexical",
      "@fields": ["theme_id", "name"]
    },
    "name": "xsd:string",
    "theme_id": "xsd:positiveInteger",
    "parent": {
      "@type": "Optional",
      "@class": "Theme"
    }
  }
]
